<?xml version="1.0" encoding="utf-8"?>
<resources>

    <string name="app_name"></string>
    <string name="app_desc"></string>
    <string name="custom_density">360</string>
    <!--已逐渐弃用，后续都是用 mdTheme的颜色值-->
    <!--主色调 -->
    <color name="colorPrimary">@color/md_theme_primary</color>
    <!--状态栏颜色-->
    <color name="colorPrimaryDark">@color/md_theme_primary</color>
    <color name="colorPrimaryLight">@color/md_theme_primaryContainer</color>
    <color name="colorOnPrimary">@color/md_theme_onPrimary</color>
    <color name="navigationBarColor">@color/md_theme_surface</color>
    <!--控制各个控件被选中时的颜色-->
    <color name="colorAccent">@color/md_theme_tertiary</color>
    <!--各个控制控件的默认颜色 与colorAccent相反-->
    <color name="colorControlNormal">@color/md_theme_onTertiary</color>
    <!--控制normal状态下button的颜色-->
    <color name="colorButtonNormal">@color/md_theme_tertiary</color>
    <!--控制所有可点击的View的涟漪效果颜色-->
    <color name="colorControlActivated">@color/md_theme_onTertiaryContainer</color>
    <color name="colorControlHighlight">@color/md_theme_tertiaryContainer</color>
    <!--页面背景色-->
    <color name="colorBackground">@color/md_theme_surface</color>
    <color name="windowBackground">@color/md_theme_surface</color>
    <color name="colorOnBackground">@color/md_theme_onSurface</color>
    <!--文本颜色-->
    <color name="textColorPrimary">@color/md_theme_onSurface</color>
    <color name="textColorSecondary">@color/md_theme_onSurfaceVariant</color>

    <!--页面背景色-->
    <color name="color_on_primary">@color/md_theme_onPrimary</color>
    <color name="window_background">@color/md_theme_surface</color>
    <color name="color_on_window">@color/md_theme_onPrimaryContainer</color>


</resources>
